#center{
  padding-bottom: 20rpx;
  width: 54%;
  margin-left: 23%;
  margin-top: 80rpx;
}
.big_func{
  width: 100%;
  height: 90rpx;
  margin-top: 50rpx;
  border-radius: 10rpx;
  font-size: 42rpx;
  padding-top: 24rpx;
  text-align: center;
  font-weight: bold;
  box-shadow: 5rpx 5rpx 10rpx #90989F;
  border: 2rpx solid #E9F2FA;
  background-color: white;
  color: rgb(73, 73, 73);
}
.big_func:active{
  background-color: #E9F2FA;
  box-shadow: 5rpx 5rpx 10rpx #90989F;
  color: #90989F;
}

#bottom{
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  height: 100rpx;
  background-color: #E9F2FA;
}
.li_func{
  float: left;
  margin-left: 20%;
  margin-top: 25rpx;
  height: 50rpx;
  width: 50rpx;
}
.bottom_icon{
  height: 100%;
  width: 100%;
  float: left;
}
.icon_image{
  width: 100%;
  height: 100%;
}

#change{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  bottom: 150rpx;
  left: 50rpx;
}

.tapNav {
  float: left;
  display: none;
  flex-direction: row;
  width: 100%;
}
.tapNav view{
  flex:1;
